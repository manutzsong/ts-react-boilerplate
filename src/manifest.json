{
  "name": "Laqoli - Lazada QOL Improvements",
  "short_name": "Laqoli",
  "description": "Mass announce promotion, unpaid, shipped and invite to follow, COD blacklist, bill customization, transaction analysis.",
  "version": "2.0",
  "background": {
    "scripts": [
      "background/index.ts"
    ]
  },
  "content_scripts": [
    {
      "run_at": "document_end",
      "matches": [
        "https://sellercenter.lazada.co.th/apps/order/index*"
      ],
      "js": [
        "content/new_print.ts"
      ]
    },
    {
      "run_at": "document_end",
      "matches": [
        "https://sellercenter.lazada.co.th/apps/order/list*"
      ],
      "js": [
        "content/newestPrint.ts"
      ]
    },
    {
      "run_at": "document_end",
      "matches": [
        "https://sellercenter.lazada.co.th/apps/order/detail*"
      ],
      "js": [
        "content/orderDetails.ts"
      ]
    },
    {
      "run_at": "document_end",
      "matches": [
        "https://sellercenter.lazada.co.th/apps/finance/accountStatement/index*"
      ],
      "js": [
        "content/transactions.ts"
      ]
    }
  ],
  "browser_action": {
    "default_popup": "pages/popup/index.html"
  },
  "icons": {
    "16": "assets/icon16.png",
    "48": "assets/icon48.png",
    "128": "assets/icon128.png"
  },
  "options_ui": {
    "page": "pages/options/index.html",
    "open_in_tab": true
  },
  "permissions": [
    "storage",
    "unlimitedStorage",
    "tabs",
    "https://sellercenter.lazada.co.th*"
  ]
}