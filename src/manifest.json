{
  "background": { "scripts": ["background/index.ts"] },
  "content_scripts": [
    {
      "js": ["content/index.ts"],
      "matches": ["https://*/*", "http://*/*"]
    },
    {
      "run_at": "document_end",
      "matches": ["https://sellercenter.lazada.co.th/apps/order/index*"],
      "js": ["content/new_print.ts"]
    },
    {
      "run_at": "document_end",
      "matches": ["https://sellercenter.lazada.co.th/apps/order/list*"],
      "js": ["content/newestPrint.ts"]
    },
    {
      "run_at": "document_end",
      "matches" : ["https://sellercenter.lazada.co.th/apps/order/detail*"],
      "js": ["content/orderDetails.ts"]
    },
    {
      "run_at": "document_end",
      "matches" : ["https://sellercenter.lazada.co.th/apps/finance/accountStatement/index*"],
      "js": ["content/transactions.ts"]
    }
  ],
  "browser_action": { "default_popup": "pages/popup/index.html" },
  "key": "",
  "icons": {
    "16": "assets/icon16.png",
    "48": "assets/icon48.png",
    "128": "assets/icon128.png"
  },
  "options_ui": {
    "page": "pages/options/index.html",
    "open_in_tab": true
  },
  "permissions": [
    "storage",
    "unlimitedStorage",
    "webRequest",
    "webRequestBlocking",
    "*://*.alicdn.com/*"
  ]
}
